<?xml version="1.0" encoding="UTF-8"?>
<OpenSearchDescription xmlns="http://a9.com/-/spec/opensearch/1.1/">
    <ShortName>vidioo</ShortName>
    <LongName>vidioo Search</LongName>
    <Description>Search for videos and channels on vidioo</Description>
    <InputEncoding>UTF-8</InputEncoding>
    <Image width="48" height="48" type="image/x-icon">https://api.vidioo.ir/favicon.ico</Image>
    <Url method="get" rel="results" type="text/html" template="https://api.vidioo.ir/results?search_query={searchTerms}" />
    <Url method="get" rel="suggestions" type="application/x-suggestions+json" template="https://api.vidioo.ir/opensearch/suggestions?query={searchTerms}" />
</OpenSearchDescription>
